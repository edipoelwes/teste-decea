{
    "openapi": "3.0.0",
    "info": {
        "title": "Example API",
        "contact": {
            "name": "Support Team",
            "email": "support@example.com"
        },
        "version": "1.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "login"
                ],
                "summary": "Sign in",
                "description": "Login by email, password",
                "operationId": "dd51a484b7fa0f97c7680fdbde14cf5f",
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "exemplo@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "123456"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/me": {
            "get": {
                "tags": [
                    "me"
                ],
                "summary": "Returns a list of courses",
                "description": "Returns a object of courses",
                "operationId": "8367feff40ba10e43187792cad3132c1",
                "responses": {
                    "200": {
                        "description": "A list with courses"
                    }
                }
            }
        },
        "/api/auth/register": {
            "post": {
                "tags": [
                    "register"
                ],
                "summary": "Sign up",
                "description": "Register",
                "requestBody": {
                    "description": "Values for register",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password",
                                    "name",
                                    "document",
                                    "social_name",
                                    "alias_name",
                                    "document_company",
                                    "document_company_secondary"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "user1"
                                    },
                                    "document": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "03585965236"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "exemplo2@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "123456"
                                    },
                                    "social_name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "Fundacao STDP LTDA"
                                    },
                                    "alias_name": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "Fundacao STDP"
                                    },
                                    "document_company": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "63565720000125"
                                    },
                                    "document_company_secondary": {
                                        "type": "string",
                                        "format": "text",
                                        "example": "5066485-8"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, Unauthorized"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}